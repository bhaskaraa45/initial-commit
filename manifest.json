{
    "name": "Initial Commit",
    "version": "1.0.1",
    "manifest_version": 3,
    "description": "Finds the intial commit of a public github repository",
    "permissions": [
      "contextMenus",
      "activeTab",
      "scripting"
    ],
    "icons": {
      "16": "/assets/icon.png",
      "48": "/assets/icon.png",
      "128": "/assets/icon.png"
    },
    "action": {
      "default_title": "Git First",
      "default_icon": "/assets/icon.png"
    },
    "background": {
      "service_worker": "index.js"
    }
}